<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CodeInput">
        <attr name="underline_color" format="color" />
        <attr name="underline_selected_color" format="color" />
        <attr name="hint_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="hint_text" format="string" />
        <attr name="codes" format="integer" />
        <attr name="input_type">
            <flag name="number" value="1" />
            <flag name="letter" value="2" />
        </attr>
    </declare-styleable>
</resources>
